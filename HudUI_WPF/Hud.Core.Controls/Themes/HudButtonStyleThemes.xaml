<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Hud.Core.Controls.Themes"
                    xmlns:controls="clr-namespace:Hud.Core.Controls">
    <Style x:Key="HudDefaultButtonStyle" TargetType="{x:Type controls:HudButton}">
        <Setter Property="OverridesDefaultStyle" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type controls:HudButton}">
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="HudProgressButtonStyle" BasedOn="{StaticResource HudDefaultButtonStyle}">
        
    </Style>
</ResourceDictionary>